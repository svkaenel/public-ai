# Environment variables for docker-compose.yml
# Copy this file to .env and modify as needed

# Port configuration
SSE_PORT=5561

# Database paths (separate for each service)
DB_PATH=./run/sse/db  # For SSE server
STDIO_DB_PATH=./run/stdio/db  # For STDIO server

# Configuration file paths
SSE_CONFIG_PATH=./run/sse/appsettings.json
STDIO_CONFIG_PATH=./run/stdio/appsettings.json
{
    "Logging": {
        "LogLevel": {
            "Default": "Error"
        },
        "Console": {
            "LogLevel": {
                "Default": "Error"
            }
        },
        "Debug": {
            "LogLevel": {
                "Default": "Debug"
            }
        }
    },
    "UseConsoleLogging": false,
    "ConnectionStrings": {
      "SupportWizardDB": "Filename=/app/db/ev-supportwizard.db"
    },
    "Embeddings": {
        "ProviderName": "Ollama",
        "DefaultModel": "nomic-embed-text",
        "Endpoint": "http://host.docker.internal:11434",
        "ChunkSize": 1000,
        "ChunkOverlap": 200
    },
    "Qdrant": {
        "QdrantEndpoint": "host.docker.internal",
        "VectorCollectionName": "ev_support_documents",
        "QdrantPort": 6334,
        "SearchLimit": 10,
        "MinimumScore": 0.5,
        "VectorDimension": 768
    },
    "AutoMigrateDatabase": true,
    "SystemName": "sdio-mcp-server"
}
{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.EntityFrameworkCore.Database.Command": "Warning",
            "Microsoft.EntityFrameworkCore.Infrastructure": "Warning"
        },
        "Console": {
            "LogLevel": {
                "Default": "Information",
                "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
            }
        },
        "Debug": {
            "LogLevel": {
                "Default": "Debug"
            }
        }
    },
    "ConnectionStrings": {
        "SupportWizardDB": "Filename=/app/db/ev-supportwizard.db"
    },
    "Embeddings": {
        "ProviderName": "Ollama",
        "DefaultModel": "nomic-embed-text",
        "Endpoint": "http://host.docker.internal:11434",
        "ChunkSize": 1000,
        "ChunkOverlap": 200
    },
    "Qdrant": {
        "QdrantEndpoint": "host.docker.internal",
        "VectorCollectionName": "ev_support_documents",
        "QdrantPort": 6334,
        "SearchLimit": 10,
        "MinimumScore": 0.5,
        "VectorDimension": 768
    },
    "AutoMigrateDatabase": true,
    "SystemName": "sse-mcp-server",
    "SSEListenUrls": "http://0.0.0.0:5561"
}